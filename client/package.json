{
  "name": "vue-media-annotator",
  "version": "1.9.5",
  "author": {
    "name": "Kitware, Inc.",
    "email": "viame-web@kitware.com"
  },
  "description": "DIVE annotation platform",
  "homepage": "https://kitware.github.io/dive",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vite --config vite.config.web.ts",
    "serve:electron": "vite build --config vite.config.electron.ts && vite-plugin-electron serve",
    "build:web": "vite build --config vite.config.web.ts",
    "build:electron": "vite build --config vite.config.electron.ts && electron-builder",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:cli": "tsc -b tsconfig.cli.json && tsc-alias -p tsconfig.cli.json",
    "dev:cli": "tsc -w -p tsconfig.cli.json & tsc-alias -w -p tsconfig.cli.json",
    "divecli": "node ./bin/platform/desktop/backend/cli.js",
    "lint": "eslint src/ dive-common/ platform/",
    "lint:templates": "eslint --ext vue src/ dive-common/ platform/",
    "test": "vitest"
  },
  "resolutions": {
    "electron-builder": "^23.0.2"
  },
  "files": [
    "/src",
    "/bin/",
    "/lib/"
  ],
  "bin": {
    "divecli": "./bin/platform/desktop/backend/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "dependencies": {
    "@flatten-js/interval-tree": "^1.0.11",
    "@girder/components": "^3.2.0",
    "@mdi/font": "^6.2.95",
    "@rollup/plugin-typescript": "^11.1.6",
    "@sentry/browser": "^5.24.2",
    "@sentry/integrations": "^5.24.2",
    "axios": "^1.5.0",
    "color-hash": "^1.0.3",
    "core-js": "^3.22.2",
    "csv-stringify": "^5.6.0",
    "d3": "^5.12.0",
    "electron": "^30.0.6",
    "geojs": "~1.6.2",
    "glob-to-regexp": "^0.4.1",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "semver": "^7.3.5",
    "vite-plugin-electron": "^0.28.7",
    "vue": "^2.7.16",
    "vue-gtag": "^1.9.1",
    "vue-router": "^3.0.3",
    "vuetify": "^2.3.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.2",
    "@sentry/vite-plugin": "^2.18.0",
    "@sentry/webpack-plugin": "^2.16.1",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/color-hash": "^1.0.0",
    "@types/cors": "^2.8.9",
    "@types/csv-parse": "^1.2.2",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.9",
    "@types/geojson": "^7946.0.7",
    "@types/glob-to-regexp": "^0.4.0",
    "@types/jest": "^27.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.151",
    "@types/mime-types": "^2.1.0",
    "@types/mock-fs": "^4.13.1",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^18.16.0",
    "@types/proper-lockfile": "^4.1.1",
    "@types/pump": "^1.1.0",
    "@types/range-parser": "^1.2.3",
    "@types/request": "^2.48.5",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/semver": "^7.3.8",
    "@types/source-map": "0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/eslint-config-airbnb": "^7.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^9.1.3",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.13.1",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^9.26.0",
    "express": "^4.17.1",
    "ffmpeg-ffprobe-static": "^4.4.0-rc.11",
    "fs-extra": "^9.0.1",
    "git-describe": "^4.1.1",
    "jest": "^27.0.4",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.27",
    "mock-fs": "^5.2.0",
    "proper-lockfile": "^4.1.1",
    "pump": "^3.0.0",
    "range-parser": "^1.2.1",
    "request": "^2.88.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "~1.32.6",
    "sass-loader": "^8.0.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^8.4.0",
    "tsc-alias": "^1.2.0",
    "tslib": "^2.6.3",
    "typescript": "~4.3.5",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.5.1",
    "vite-plugin-vue2": "^2.0.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^0.29.2",
    "vue-template-compiler": "^2.7.16",
    "xml-js": "^1.6.11",
    "yargs": "^16.1.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^vue-media-annotator/(.*)$": "<rootDir>/src/$1",
      "^dive-common/(.*)$": "<rootDir>/dive-common/$1",
      "^platform/(.*)$": "<rootDir>/platform/$1",
      "\\.css$": "<rootDir>/test/stub.js"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/dist_electron/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "no-underscore-dangle": 0,
      "spaced-comment": "off",
      "no-useless-constructor": "off",
      "no-use-before-define": "off",
      "vue/require-valid-default-prop": "off",
      "vue/require-default-prop": "off",
      "vue/no-unused-components": "off",
      "vue/valid-v-slot": "off",
      "vuejs-accessibility/alt-text": "off",
      "vue/no-v-text-v-html-on-component": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "vue/no-template-target-blank": "off",
      "vue/no-lone-template": "off",
      "vuejs-accessibility/mouse-events-have-key-events": "off",
      "vuejs-accessibility/click-events-have-key-events": "off",
      "vue/no-mutating-props": "off",
      "no-promise-executor-return": "off",
      "no-shadow": "off",
      "vue/multi-word-component-names": "off",
      "vuejs-accessibility/form-control-has-label": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-useless-constructor": "error",
      "import/extensions": "off",
      "vue/max-len": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "semi": "off",
      "@typescript-eslint/semi": [
        "error"
      ]
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.vue"
        ],
        "extends": [
          "@vue/typescript",
          "@vue/typescript/recommended"
        ]
      }
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2020
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
